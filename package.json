{
  "name": "ts-tiny-invariant",
  "version": "2.0.2",
  "description": "Stricter version of tiny-invariant",
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "react-native": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "preversion": "npm run lint && npm run build && npm t",
    "postversion": "git push && git push --tags",
    "build": "rm -rf dist && rollup -c rollup.config.ts --configPlugin typescript && rm -rf tmp",
    "lint": "tslint -p .",
    "watch": "tsc -p . -w",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "invariant"
  ],
  "files": [
    "dist",
    "src",
    "test"
  ],
  "author": "iyegoroff <iegoroff@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iyegoroff/ts-tiny-invariant/issues"
  },
  "homepage": "https://github.com/iyegoroff/ts-tiny-invariant#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iyegoroff/ts-tiny-invariant.git"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.33",
    "jest": "^28.1.0",
    "rollup": "^2.73.0",
    "rollup-plugin-dts": "^4.2.1",
    "ts-jest": "^28.0.2",
    "tslint": "^6.1.3",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testEnvironment": "node"
  }
}
